package javahong;


public interface Test {
	public double perimeter();                  //인터페이스, 추상 메소드
	public float area();
}

//인터페이스로 만든 클래스에 메소드 오버로딩을 해야한다.
//상속 (필드, 생성자, 메서드 super)

class Tri implements Test {  
        //필드
	    private int x1, x2, x3, y1, y2, y3;
	    //생성자
	    public Tri(int x1, int x2, int x3, int y1, int y2, int y3) {
	    	this.x1=x1; this.y1=y1;
	    	this.x2=x2; this.y2=y2;
	    	this.x3=x3; this.y3=y3;
	    }
	    // 메서드
	    public double perimeter() {
	    	
	    	
	    	;
	    }
	    public float area() {
	    	;
	    	
	  
	   }


}
