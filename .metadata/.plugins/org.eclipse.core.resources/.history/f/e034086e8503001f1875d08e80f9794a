package ch08;

enum EnumTest{
	 READY ,SEND,COMPLETE, CLOSE
}




public class NonEnumTest {

	public NonEnumTest() {
		// TODO Auto-generated constructor stub
	}
	
	public static void main(String [] args) {
		EnumTest work =null ;
		int n = 2;
		
		
		switch(n) {
		case 1 : 
			work = EnumTest.READY;
			break;
		case 2 : 
			work = EnumTest.SEND;
			break;
		case 3 : 
			work = EnumTest.COMPLETE;
			break;
		case 4 : 
			work = EnumTest.CLOSE;
			break;
		}
		System.out.println("현재 작업 상태: " + work);
	}

}
