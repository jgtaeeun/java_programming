package com.ruby.java.ch08.nonEnum;

enum Status2 {
	 READY, SEND , COMPLETE , CLOSE  ;
}




public class NonEnumTest2 {

	public NonEnumTest2() {
		// TODO Auto-generated constructor stub
	}
	
	public static void main(String [] args) {
		Status2 work =null ;
		int n = 2;
		
		
		switch(n) {
		case 1 : 
			work = Status2.READY;
			break;
		case 2 : 
			work = Status2.SEND;
			break;
		case 3 : 
			work = Status2.COMPLETE;
			break;
		case 4 : 
			work = Status2.CLOSE;
			break;
		}
		System.out.println("현재 작업 상태: " + work);
	}

}
